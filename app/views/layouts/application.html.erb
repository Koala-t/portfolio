<!DOCTYPE html>
<html>
  <head>
    <title>Portfolio</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <link href='//fonts.googleapis.com/css?family=Alegreya Sans SC' rel='stylesheet'>
    
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render 'partials/navbar'%>
    <%= render 'partials/bio' %>
    <%= yield %>

    <div class="background" id="experience">
      <div class="sectionHeading">
        <h1>- My Background -</h1>
      </div>
      <div class="previous_employers">
        <%= render 'partials/maxPlanck' %>
        <%= render 'partials/alexion' %>
      </div>
      <div class="education">
        <%= render 'partials/coloradoCollege' %>
        <%= render 'partials/devBootcamp' %>
      </div>
    </div>

  </body>
</html>
